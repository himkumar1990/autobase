properties([
	pipelineTriggers([
		upstream(
		   threshold: hudson.model.Result.SUCCESS,
		   upstreamProjects : 'test1'
                )

	])

])

properties([pipelineTriggers([githubPush()])])



pipeline {

	agent any 
	
	stages {
		stage('Compile Stage'){
			steps{
				sh 'echo compile step'
				input 'continue to next stage ?'
			}
		}

		
		stage('Testing Stage'){
			steps{
				sh 'echo test step'
			}
		}
	}
}
