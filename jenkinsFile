properties([
	pipelineTriggers([
		upstream(
		   threshold: hudson.model.Result.SUCCESS,
		   upstreamProjects : 'test1'
                )

	])

])

properties([pipelineTriggers([cron('H/5 * * * *')])])


pipeline {

	agent any 
	
	stages {
		stage('Compile Stage'){
			steps{
				sh 'echo compile step'
			}
		}
		stage('Testing Stage'){
			steps{
				sh 'echo test step'
			}
		}
	}
}
